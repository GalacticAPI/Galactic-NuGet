    <metadata>
        <id>Galactic.Configuration</id>
        <version>[VERSION]</version>
        <authors>Galactic API Team</authors>
        <licenseUrl>https://raw.githubusercontent.com/GalacticAPI/Galactic/master/LICENSE</licenseUrl>
